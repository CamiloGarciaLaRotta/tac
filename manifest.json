{
  "manifest_version": 2,
  "name": "Tack Chrome Extension",
  "description": "Keep track of applications ad-hoc",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "web_accessible_resources": [
    "data/*.json"
  ]
}
